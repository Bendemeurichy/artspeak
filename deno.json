{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.5",
    "@emotion/react": "npm:@emotion/react@^11.14.0",
    "@emotion/styled": "npm:@emotion/styled@^11.14.1",
    "@fontsource/roboto": "npm:@fontsource/roboto@^5.2.9",
    "@mui/icons-material": "npm:@mui/icons-material@^7.3.7",
    "@mui/material": "npm:@mui/material@^7.3.7",
    "@types/react": "npm:@types/react@^19.2.7",
    "@types/react-dom": "npm:@types/react-dom@^19.2.3",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^5.1.2",
    "motion": "npm:motion@^12.28.1",
    "react": "npm:react@^19.2.1",
    "react-dom": "npm:react-dom@^19.2.1",
    "react-router": "npm:react-router@^7.12.0",
    "@react-router/dev/vite": "npm:@react-router/dev@^7.12.0/vite",
    "@react-router/dev/routes": "npm:@react-router/dev@^7.12.0/routes",
    "@react-router/dev/config": "npm:@react-router/dev@^7.12.0/config",
    "@react-router/fs-routes": "npm:@react-router/fs-routes@^7.12.0",
    "tts-react": "npm:tts-react@^4.1.2",
    "vite": "npm:vite@^7.2.7"
  }
}
